	<element name="Publication" type="aqd:PublicationType" substitutionGroup="gml:AbstractObject">
		<annotation>
			<documentation>-- Name --
publication

-- Definition --
Documentation of the publication of the AQ Plan.</documentation>
		</annotation>
	</element>
	<complexType name="PublicationType">
		<sequence>
			<element name="description" type="string">
				<annotation>
					<documentation>-- Name --
description

-- Definition --
Title as written in the publication.</documentation>
				</annotation>
			</element>
			<element name="title" type="string">
				<annotation>
					<documentation>-- Name --
title

-- Definition --
Short description of the publication. ISBN number should be provided if available.</documentation>
				</annotation>
			</element>
			<element name="author" type="string" minOccurs="0">
				<annotation>
					<documentation>-- Name --
author

-- Definition --
If there are multiple authors, please provide in one field separated by commas</documentation>
				</annotation>
			</element>
			<element name="publicationDate" type="gml:TimeInstantPropertyType">
				<annotation>
					<documentation>-- Name --
publicationDate

-- Definition --
